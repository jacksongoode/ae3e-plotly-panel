{
  "name": "nline-plotlyjs-panel",
  "version": "1.2.0",
  "description": "Grafana Plotly Plugin",
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "sign": "export $(xargs <keys.env) && npx @grafana/toolkit plugin:sign --rootUrls http://localhost:3000",
    "pack": "./package.sh",
    "md5": "md5 nline-plotlyjs-panel-%npm_package_version%.zip nline-plotlyjs-panel-%npm_package_version%.zip.md5",
    "docker": "docker run -d -p 3000:3000 -v \"$(pwd)\":/var/lib/grafana/plugins --name=grafana grafana/grafana:8.3.4"
  },
  "author": "nLine",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/preset-env": "7.5.5",
    "@grafana/data": "8.3.4",
    "@grafana/runtime": "8.3.4",
    "@grafana/toolkit": "8.3.4",
    "@grafana/ui": "8.3.4"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "file-saver": "^2.0.5",
    "js-yaml": "^4.1.0",
    "plotly.js": "^2.16.3",
    "re-resizable": "^6.9.9",
    "react-plotly.js": "^2.6.0"
  },
  "resolutions": {
    "@babel/preset-env": "7.5.5"
  }
}