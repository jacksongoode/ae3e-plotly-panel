{
  "name": "nline-plotlyjs-panel",
  "version": "1.6.5",
  "description": "Grafana Plotly Plugin",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "docker": "docker run -d -p 3000:3000 -v \"$(pwd)\":/var/lib/grafana/plugins --name=grafana grafana/grafana",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "md5": "md5 nline-plotlyjs-panel-%npm_package_version%.zip nline-plotlyjs-panel-%npm_package_version%.zip.md5",
    "package": "./package.sh",
    "release": "tag=$(git describe --abbrev=0) && gh release upload ${tag} ./nline-plotlyjs-panel-${tag}.zip ./nline-plotlyjs-panel-${tag}.zip.md5 --clobber",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "validate": "tag=$(git describe --abbrev=0) && npx -y @grafana/plugin-validator@latest -sourceCodeUri https://github.com/nline/nline-plotlyjs-panel/tree/${tag} https://github.com/nline/nline-plotlyjs-panel/releases/download/${tag}/nline-plotlyjs-panel-${tag}.zip"
  },
  "author": "nLine",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@grafana/e2e": "10.4.1",
    "@grafana/e2e-selectors": "10.4.1",
    "@grafana/eslint-config": "^6.0.1",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@swc/core": "^1.3.102",
    "@swc/helpers": "^0.5.3",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "6.2.0",
    "@testing-library/react": "14.1.2",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.7",
    "@types/plotly.js-dist-min": "^2.3.4",
    "@types/react-plotly.js": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-jsdoc": "^48.0.2",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^10.3.10",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.1",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.69.7",
    "sass-loader": "13.3.3",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "@types/testing-library__jest-dom": "5.14.9"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@grafana/data": "^10.4.1",
    "@grafana/runtime": "^10.4.1",
    "@grafana/ui": "^10.4.1",
    "@types/file-saver": "^2.0.7",
    "@types/plotly.js": "^2.12.32",
    "dayjs": "^1.11.10",
    "deepmerge": "^4.3.1",
    "file-saver": "^2.0.5",
    "js-yaml": "^4.1.0",
    "plotly.js-dist-min": "^2.28.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-plotly.js": "^2.6.0",
    "react-resizable": "^3.0.5",
    "tslib": "2.6.2"
  },
  "packageManager": "yarn@1.22.19"
}
